                        /* @__PURE__ */ jsxRuntimeExports.jsx(SelectTrigger, { children: /* @__PURE__ */ jsxRuntimeExports.jsx(SelectValue, {}) }),
                        /* @__PURE__ */ jsxRuntimeExports.jsx(SelectContent, { children: expenseCategories.map((cat) => /* @__PURE__ */ jsxRuntimeExports.jsx(SelectItem, { value: cat.id, children: cat.name }, cat.id)) })
                      ]
                    }
                  )
                ] }),
                /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { children: [
                  /* @__PURE__ */ jsxRuntimeExports.jsx("label", { className: "text-sm font-medium text-muted-foreground", children: "Hn mc" }),
                  /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "relative", children: [
                    /* @__PURE__ */ jsxRuntimeExports.jsx(
                      Input,
                      {
                        type: "text",
                        inputMode: "numeric",
                        placeholder: "0",
                        value: newBudget.limit,
                        onChange: (e) => setNewBudget((prev) => ({ ...prev, limit: formatInputAmount(e.target.value) })),
                        className: "pr-16"
                      }
                    ),
                    /* @__PURE__ */ jsxRuntimeExports.jsx("span", { className: "absolute right-4 top-1/2 -translate-y-1/2 text-muted-foreground text-sm", children: "VND" })
                  ] })
                ] }),
                /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { children: [
                  /* @__PURE__ */ jsxRuntimeExports.jsx("label", { className: "text-sm font-medium text-muted-foreground", children: "Chu k" }),
                  /* @__PURE__ */ jsxRuntimeExports.jsxs(
                    Select,
                    {
                      value: newBudget.period,
                      onValueChange: (v) => setNewBudget((prev) => ({ ...prev, period: v })),
                      children: [
                        /* @__PURE__ */ jsxRuntimeExports.jsx(SelectTrigger, { children: /* @__PURE__ */ jsxRuntimeExports.jsx(SelectValue, {}) }),
                        /* @__PURE__ */ jsxRuntimeExports.jsxs(SelectContent, { children: [
                          /* @__PURE__ */ jsxRuntimeExports.jsx(SelectItem, { value: "weekly", children: "Hng tun" }),
                          /* @__PURE__ */ jsxRuntimeExports.jsx(SelectItem, { value: "monthly", children: "Hng thng" })
                        ] })
                      ]
                    }
                  )
                ] }),
                /* @__PURE__ */ jsxRuntimeExports.jsx(Button, { onClick: handleAddBudget, className: "w-full gradient-primary", children: "Thm ngn sch" })
              ] })
            ] })
          ] })
        ] }),
        budgets.length === 0 ? /* @__PURE__ */ jsxRuntimeExports.jsx("div", { className: "bg-secondary rounded-xl p-4 text-center", children: /* @__PURE__ */ jsxRuntimeExports.jsx("p", { className: "text-sm text-muted-foreground", children: "Cha c ngn sch no" }) }) : /* @__PURE__ */ jsxRuntimeExports.jsx("div", { className: "space-y-2", children: budgets.map((budget) => {
          const category = CATEGORIES.find((c) => c.id === budget.category);
          return /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "flex items-center justify-between bg-card rounded-xl p-3 border border-border/50", children: [
            /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { children: [
              /* @__PURE__ */ jsxRuntimeExports.jsx("p", { className: "font-medium text-sm", children: category?.name }),
              /* @__PURE__ */ jsxRuntimeExports.jsxs("p", { className: "text-xs text-muted-foreground", children: [
                formatCurrency(budget.limit),
                " / ",
                budget.period === "weekly" ? "tun" : "thng"
              ] })
            ] }),
            /* @__PURE__ */ jsxRuntimeExports.jsx(
              "button",
              {
                onClick: () => {
                  deleteBudget(budget.id);
                  ue$1.success(" xa ngn sch");
                },
                className: "p-2 text-muted-foreground hover:text-destructive transition-colors",
                children: /* @__PURE__ */ jsxRuntimeExports.jsx(Trash2, { className: "w-4 h-4" })
              }
            )
          ] }, budget.id);
        }) })
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs("section", { children: [
        /* @__PURE__ */ jsxRuntimeExports.jsxs("h2", { className: "font-semibold flex items-center gap-2 mb-3", children: [
          /* @__PURE__ */ jsxRuntimeExports.jsx(Bell, { className: "w-4 h-4 text-accent-purple" }),
          "Thng bo"
        ] }),
        /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "bg-card rounded-xl border border-border/50 divide-y divide-border/50", children: [
          /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "flex items-center justify-between p-4", children: [
            /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { children: [
              /* @__PURE__ */ jsxRuntimeExports.jsx("p", { className: "font-medium text-sm", children: "Cnh bo vt ngn sch" }),
              /* @__PURE__ */ jsxRuntimeExports.jsx("p", { className: "text-xs text-muted-foreground", children: "Nhn thng bo khi chi tiu vt hn mc" })
            ] }),
            /* @__PURE__ */ jsxRuntimeExports.jsx(Switch, { checked: notifications, onCheckedChange: setNotifications })
          ] }),
          /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "flex items-center justify-between p-4", children: [
            /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { children: [
              /* @__PURE__ */ jsxRuntimeExports.jsx("p", { className: "font-medium text-sm", children: "Nhc nh hng ngy" }),
              /* @__PURE__ */ jsxRuntimeExports.jsx("p", { className: "text-xs text-muted-foreground", children: "Nhc nh ghi chp chi tiu" })
            ] }),
            /* @__PURE__ */ jsxRuntimeExports.jsx(Switch, {})
          ] })
        ] })
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs("section", { children: [
        /* @__PURE__ */ jsxRuntimeExports.jsxs("h2", { className: "font-semibold flex items-center gap-2 mb-3", children: [
          /* @__PURE__ */ jsxRuntimeExports.jsx(Info, { className: "w-4 h-4 text-accent-blue" }),
          "Thng tin ng dng"
        ] }),
        /* @__PURE__ */ jsxRuntimeExports.jsx("div", { className: "bg-card rounded-xl border border-border/50", children: /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "flex items-center justify-between p-4", children: [
          /* @__PURE__ */ jsxRuntimeExports.jsx("span", { className: "text-sm", children: "Phin bn" }),
          /* @__PURE__ */ jsxRuntimeExports.jsx("span", { className: "text-sm text-muted-foreground", children: "1.0.1" })
        ] }) })
      ] })
    ] })
  ] });
};

const Transactions = () => {
  const navigate = useNavigate();
  const { user } = useAuth();
  const { transactions, searchTransactions, deleteTransaction, updateTransaction } = useExpenseStore();
  const [searchQuery, setSearchQuery] = reactExports.useState("");
  const [selectedCategory, setSelectedCategory] = reactExports.useState("all");
  const [deleteId, setDeleteId] = reactExports.useState(null);
  const [editingId, setEditingId] = reactExports.useState(null);
  const [editingNote, setEditingNote] = reactExports.useState("");
  let filteredTransactions = searchQuery ? searchTransactions(searchQuery) : transactions;
  if (selectedCategory !== "all") {
    filteredTransactions = filteredTransactions.filter((t) => t.category === selectedCategory);
  }
  const handleDelete = async () => {
    if (deleteId) {
      try {
        await deleteTransaction(deleteId);
        ue$1.success(" xa giao dch");
        setDeleteId(null);
      } catch (error) {
        console.error("Error deleting transaction:", error);
        ue$1.error("C li xy ra");
      }
    }
  };
  const handleEditClick = (transaction) => {
    setEditingId(transaction.id);
    setEditingNote(transaction.note || "");
  };
  const handleSaveEdit = async () => {
    if (editingId) {
      try {
        await updateTransaction(editingId, { note: editingNote });
        ue$1.success(" cp nht ghi ch");
        setEditingId(null);
        setEditingNote("");
      } catch (error) {
        console.error("Error updating transaction:", error);
        ue$1.error("C li xy ra");
      }
    }
  };
  return /* @__PURE__ */ jsxRuntimeExports.jsxs(MobileLayout, { showNav: false, children: [
    /* @__PURE__ */ jsxRuntimeExports.jsxs("header", { className: "px-4 py-4 pt-5 border-b border-border safe-area-top", style: { marginTop: "20px" }, children: [
      /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "flex items-center gap-4 mb-4", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx("button", { onClick: () => navigate(-1), className: "p-2 -ml-2 hover:bg-muted rounded-lg", children: /* @__PURE__ */ jsxRuntimeExports.jsx(ArrowLeft, { className: "w-5 h-5" }) }),
        /* @__PURE__ */ jsxRuntimeExports.jsx("h1", { className: "font-semibold text-lg", children: "V ca ti" })
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "relative", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Search$1, { className: "absolute left-3 top-1/2 -translate-y-1/2 w-4 h-4 text-muted-foreground" }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(
          Input,
          {
            placeholder: "Tm kim giao dch...",
            value: searchQuery,
            onChange: (e) => setSearchQuery(e.target.value),
            className: "pl-10"
          }
        )
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "flex gap-2 mt-3 overflow-x-auto pb-2 -mx-4 px-4", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(
          "button",
          {
            onClick: () => setSelectedCategory("all"),
            className: cn(
              "px-3 py-1.5 rounded-full text-xs font-medium whitespace-nowrap transition-colors",
              selectedCategory === "all" ? "bg-primary text-primary-foreground" : "bg-secondary text-muted-foreground"
            ),
            children: "Tt c"
          }
        ),
        CATEGORIES.map((cat) => /* @__PURE__ */ jsxRuntimeExports.jsx(
          "button",
          {
            onClick: () => setSelectedCategory(cat.id),
            className: cn(
              "px-3 py-1.5 rounded-full text-xs font-medium whitespace-nowrap transition-colors",
              selectedCategory === cat.id ? "bg-primary text-primary-foreground" : "bg-secondary text-muted-foreground"
            ),
            children: cat.name
          },
          cat.id
        ))
      ] })
    ] }),
    /* @__PURE__ */ jsxRuntimeExports.jsx("div", { className: "flex-1 overflow-auto pb-20", children: filteredTransactions.length === 0 ? /* @__PURE__ */ jsxRuntimeExports.jsx("div", { className: "text-center py-12", children: /* @__PURE__ */ jsxRuntimeExports.jsx("p", { className: "text-muted-foreground", children: "Khng tm thy giao dch no" }) }) : /* @__PURE__ */ jsxRuntimeExports.jsx("div", { className: "divide-y divide-border/50", children: filteredTransactions.map((transaction) => /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "relative group", children: [
      /* @__PURE__ */ jsxRuntimeExports.jsx(TransactionItem, { transaction }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "absolute right-4 top-1/2 -translate-y-1/2 flex gap-2 opacity-0 group-hover:opacity-100 transition-opacity", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(
          "button",
          {
            onClick: () => handleEditClick(transaction),
            className: "p-2 text-muted-foreground hover:text-primary rounded-lg hover:bg-muted",
            children: /* @__PURE__ */ jsxRuntimeExports.jsx(Pen, { className: "w-4 h-4" })
          }
        ),
        /* @__PURE__ */ jsxRuntimeExports.jsx(
          "button",
          {
            onClick: () => setDeleteId(transaction.id),
            className: "p-2 text-muted-foreground hover:text-destructive rounded-lg hover:bg-muted",
            children: /* @__PURE__ */ jsxRuntimeExports.jsx(Trash2, { className: "w-4 h-4" })
          }
        )
      ] })
    ] }, transaction.id)) }) }),
    /* @__PURE__ */ jsxRuntimeExports.jsx(Dialog, { open: !!deleteId, onOpenChange: () => setDeleteId(null), children: /* @__PURE__ */ jsxRuntimeExports.jsxs(DialogContent, { className: "max-w-[90vw] rounded-2xl", children: [
      /* @__PURE__ */ jsxRuntimeExports.jsxs(DialogHeader, { children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(DialogTitle, { children: "Xa giao dch" }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(DialogDescription, { children: "Bn c chc chn mun xa giao dch ny? Hnh ng ny khng th hon tc." })
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs(DialogFooter, { className: "flex-row gap-2", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Button, { variant: "outline", onClick: () => setDeleteId(null), className: "flex-1", children: "Hy" }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(Button, { variant: "destructive", onClick: handleDelete, className: "flex-1", children: "Xa" })
      ] })
    ] }) }),
    /* @__PURE__ */ jsxRuntimeExports.jsx(Dialog, { open: !!editingId, onOpenChange: () => {
      setEditingId(null);
      setEditingNote("");
    }, children: /* @__PURE__ */ jsxRuntimeExports.jsxs(DialogContent, { className: "max-w-[90vw] rounded-2xl", children: [
      /* @__PURE__ */ jsxRuntimeExports.jsxs(DialogHeader, { children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(DialogTitle, { children: "Sa ghi ch" }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(DialogDescription, { children: "Cp nht ghi ch cho giao dch ny" })
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsx("div", { className: "space-y-4", children: /* @__PURE__ */ jsxRuntimeExports.jsx(
        Textarea,
        {
          placeholder: "Nhp ghi ch...",
          value: editingNote,
          onChange: (e) => setEditingNote(e.target.value),
          rows: 4,
          className: "resize-none"
        }
      ) }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs(DialogFooter, { className: "flex-row gap-2", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(
          Button,
          {
            variant: "outline",
            onClick: () => {
              setEditingId(null);
              setEditingNote("");
            },
            className: "flex-1",
            children: "Hy"
          }
        ),
        /* @__PURE__ */ jsxRuntimeExports.jsx(
          Button,
          {
            onClick: handleSaveEdit,
            className: "flex-1",
            children: "Lu"
          }
        )
      ] })
    ] }) })
  ] });
};

const InitialBalance = () => {
  const navigate = useNavigate();
  const { transactions, addTransaction, updateTransaction, deleteTransaction } = useExpenseStore();
  const [initialBalances, setInitialBalances] = reactExports.useState([]);
  const [isAddDialogOpen, setIsAddDialogOpen] = reactExports.useState(false);
  const [isEditDialogOpen, setIsEditDialogOpen] = reactExports.useState(false);
  const [editingId, setEditingId] = reactExports.useState(null);
  const [deleteId, setDeleteId] = reactExports.useState(null);
  const [amount, setAmount] = reactExports.useState("");
  const [description, setDescription] = reactExports.useState("");
  reactExports.useEffect(() => {
    const balances = transactions.filter((t) => t.note === "S d thng ny" && t.type === "income");
    setInitialBalances(balances);
  }, [transactions]);
  const totalInitialBalance = initialBalances.reduce((sum, b) => sum + b.amount, 0);
  const formatInputAmount = (value) => {
    const numbers = value.replace(/\D/g, "");
    return numbers.replace(/\B(?=(\d{3})+(?!\d))/g, ",");
  };
  const handleAddBalance = async () => {
    const parsedAmount = parseFloat(amount.replace(/[,.]/g, ""));
    if (!parsedAmount || parsedAmount <= 0) {
      ue$1.error("Vui lng nhp s tin hp l");
      return;
    }
    if (!description.trim()) {
      ue$1.error("Vui lng nhp m t");
      return;
    }
    try {
      await addTransaction({
        type: "income",
        amount: parsedAmount,
        category: "income",
        description: description.trim(),
        date: /* @__PURE__ */ new Date(),
        note: "S d thng ny"
      });
      ue$1.success(" thm s d");
      setIsAddDialogOpen(false);
      setAmount("");
      setDescription("");
    } catch (error) {
      console.error("Error adding balance:", error);
      ue$1.error("C li xy ra");
    }
  };
  const handleEditBalance = (balance) => {
    setEditingId(balance.id);
    setAmount(balance.amount.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ","));
    setDescription(balance.description);
    setIsEditDialogOpen(true);
  };
  const handleSaveEdit = async () => {
    if (!editingId) return;
    const parsedAmount = parseFloat(amount.replace(/[,.]/g, ""));
    if (!parsedAmount || parsedAmount <= 0) {
      ue$1.error("Vui lng nhp s tin hp l");
      return;
    }
    if (!description.trim()) {
      ue$1.error("Vui lng nhp m t");
      return;
    }
    try {
      await updateTransaction(editingId, {
        amount: parsedAmount,
        description: description.trim()
      });
      ue$1.success(" cp nht s d");
      setIsEditDialogOpen(false);
      setEditingId(null);
      setAmount("");
      setDescription("");
    } catch (error) {
      console.error("Error updating balance:", error);
      ue$1.error("C li xy ra");
    }
  };
  const handleDeleteBalance = async () => {
    if (deleteId) {
      try {
        await deleteTransaction(deleteId);
        ue$1.success(" xa s d");
        setDeleteId(null);
      } catch (error) {
        console.error("Error deleting balance:", error);
        ue$1.error("C li xy ra");
      }
    }
  };
  return /* @__PURE__ */ jsxRuntimeExports.jsxs(MobileLayout, { showNav: false, children: [
    /* @__PURE__ */ jsxRuntimeExports.jsxs("header", { className: "px-4 py-4 pt-5 border-b border-border safe-area-top", style: { marginTop: "20px" }, children: [
      /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "flex items-center gap-4 mb-4", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx("button", { onClick: () => navigate(-1), className: "p-2 -ml-2 hover:bg-muted rounded-lg", children: /* @__PURE__ */ jsxRuntimeExports.jsx(ArrowLeft, { className: "w-5 h-5" }) }),
        /* @__PURE__ */ jsxRuntimeExports.jsx("h1", { className: "font-semibold text-lg", children: "Qun l s d" })
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "gradient-primary rounded-xl p-4 text-primary-foreground mt-2", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx("p", { className: "text-xs opacity-80 mb-1", children: "Tng s d ban u" }),
        /* @__PURE__ */ jsxRuntimeExports.jsx("h2", { className: "text-2xl font-bold", children: formatCurrency(totalInitialBalance) })
      ] })
    ] }),
    /* @__PURE__ */ jsxRuntimeExports.jsx("div", { className: "flex-1 overflow-auto pb-20", children: initialBalances.length === 0 ? /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "text-center py-12", children: [
      /* @__PURE__ */ jsxRuntimeExports.jsx("p", { className: "text-muted-foreground mb-4", children: "Cha c s d ban u no" }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs(Button, { onClick: () => setIsAddDialogOpen(true), className: "gradient-primary", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Plus, { className: "w-4 h-4 mr-2" }),
        "Thm s d"
      ] })
    ] }) : /* @__PURE__ */ jsxRuntimeExports.jsx("div", { className: "divide-y divide-border/50", children: initialBalances.map((balance) => /* @__PURE__ */ jsxRuntimeExports.jsx("div", { className: "p-4 hover:bg-muted/50 transition-colors group", children: /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "flex items-start justify-between gap-3", children: [
      /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "flex-1", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx("h3", { className: "font-medium text-sm", children: balance.description }),
        /* @__PURE__ */ jsxRuntimeExports.jsx("p", { className: "text-xs text-muted-foreground mt-1", children: new Date(balance.date).toLocaleDateString("vi-VN") })
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsx("div", { className: "text-right mr-10", children: /* @__PURE__ */ jsxRuntimeExports.jsx("p", { className: "font-semibold text-success", children: formatCurrency(balance.amount) }) }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "flex gap-2 opacity-0 group-hover:opacity-100 transition-opacity", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(
          "button",
          {
            onClick: () => handleEditBalance(balance),
            className: "p-2 text-muted-foreground hover:text-primary rounded-lg hover:bg-muted",
            children: /* @__PURE__ */ jsxRuntimeExports.jsx(Pen, { className: "w-4 h-4" })
          }
        ),
        /* @__PURE__ */ jsxRuntimeExports.jsx(
          "button",
          {
            onClick: () => setDeleteId(balance.id),
            className: "p-2 text-muted-foreground hover:text-destructive rounded-lg hover:bg-muted",
            children: /* @__PURE__ */ jsxRuntimeExports.jsx(Trash2, { className: "w-4 h-4" })
          }
        )
      ] })
    ] }) }, balance.id)) }) }),
    initialBalances.length > 0 && /* @__PURE__ */ jsxRuntimeExports.jsx("div", { className: "fixed bottom-20 right-4 md:right-auto md:left-1/2 md:-translate-x-1/2", children: /* @__PURE__ */ jsxRuntimeExports.jsx(
      "button",
      {
        onClick: () => setIsAddDialogOpen(true),
        className: "w-12 h-12 rounded-full gradient-primary shadow-lg flex items-center justify-center text-primary-foreground hover:shadow-xl transition-shadow active:scale-95",
        children: /* @__PURE__ */ jsxRuntimeExports.jsx(Plus, { className: "w-6 h-6" })
      }
    ) }),
    /* @__PURE__ */ jsxRuntimeExports.jsx(Dialog, { open: isAddDialogOpen, onOpenChange: setIsAddDialogOpen, children: /* @__PURE__ */ jsxRuntimeExports.jsxs(DialogContent, { className: "max-w-[90vw] rounded-2xl", children: [
      /* @__PURE__ */ jsxRuntimeExports.jsxs(DialogHeader, { children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(DialogTitle, { children: "Thm s d mi" }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(DialogDescription, { children: "Nhp s tin v m t cho s d" })
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "space-y-4", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "space-y-2", children: [
          /* @__PURE__ */ jsxRuntimeExports.jsx("label", { className: "text-sm font-medium", children: "S tin (VND)" }),
          /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "relative", children: [
            /* @__PURE__ */ jsxRuntimeExports.jsx(
              Input,
              {
                type: "text",
                inputMode: "numeric",
                placeholder: "0",
                value: amount,
                onChange: (e) => setAmount(formatInputAmount(e.target.value)),
                className: "text-lg font-semibold pr-12"
              }
            ),
            /* @__PURE__ */ jsxRuntimeExports.jsx("span", { className: "absolute right-4 top-1/2 -translate-y-1/2 text-muted-foreground font-medium", children: "VND" })
          ] })
        ] }),
        /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "space-y-2", children: [
          /* @__PURE__ */ jsxRuntimeExports.jsx("label", { className: "text-sm font-medium", children: "M t" }),
