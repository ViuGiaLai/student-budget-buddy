	   * const { data, error, count } = await supabase
	   *   .from('users')
	   *   .upsert(
	   *     {
	   *       id: 3,
	   *       message: 'foo',
	   *       username: 'supabot'
	   *     },
	   *     {
	   *       onConflict: 'username',
	   *       count: 'exact'
	   *     }
	   *   )
	   *
	   * // Example response:
	   * // {
	   * //   data: [
	   * //     {
	   * //       id: 42,
	   * //       handle: "saoirse",
	   * //       display_name: "Saoirse"
	   * //     }
	   * //   ],
	   * //   count: 1,
	   * //   error: null
	   * // }
	   * ```
	   */
