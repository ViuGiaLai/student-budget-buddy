const normalizePath = (raw) => {
  if (!raw) return "/";
  const noHash = raw.split("#")[0];
  const noQuery = noHash.split("?")[0];
  const trimmed = noQuery.replace(/\/+$/, "");
  return trimmed || "/";
};
