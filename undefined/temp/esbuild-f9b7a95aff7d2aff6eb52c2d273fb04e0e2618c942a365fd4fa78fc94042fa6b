      name: newGoal.name.trim(),
      targetAmount: target,
      currentAmount: 0,
      deadline: newGoal.deadline ? new Date(newGoal.deadline) : void 0,
      color: newGoal.color
    });
    ue$1.success(" to mc tiu mi");
