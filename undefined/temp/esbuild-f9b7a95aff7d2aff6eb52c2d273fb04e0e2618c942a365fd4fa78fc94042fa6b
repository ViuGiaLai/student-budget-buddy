const AddTransaction = () => {
  const navigate = useNavigate();
  const [searchParams] = useSearchParams();
  const { user } = useAuth();
  const { addTransaction } = useExpenseStore();
  const initialType = searchParams.get("type") || "expense";
  const [type, setType] = reactExports.useState(initialType);
  const [amount, setAmount] = reactExports.useState("");
  const [category, setCategory] = reactExports.useState(type === "income" ? "income" : "food");
  const [description, setDescription] = reactExports.useState("");
  const [note, setNote] = reactExports.useState("");
  const expenseCategories = CATEGORIES.filter((c) => c.id !== "income");
  const incomeCategories = CATEGORIES.filter((c) => c.id === "income" || c.id === "other");
  const displayCategories = type === "income" ? incomeCategories : expenseCategories;
  const handleSubmit = async () => {
    if (!amount || parseFloat(amount) <= 0) {
      ue$1.error("Vui lng nhp s tin hp l");
      return;
    }
    if (!description.trim()) {
      ue$1.error("Vui lng nhp m t");
      return;
    }
    try {
      await addTransaction({
        type,
        amount: parseFloat(amount.replace(/[,.]/g, "")),
        category,
        description: description.trim(),
        date: /* @__PURE__ */ new Date(),
        note: note.trim()
      });
      ue$1.success(type === "income" ? " thm thu nhp" : " thm chi tiu");
      navigate("/");
    } catch (error) {
      console.error("Error adding transaction:", error);
      ue$1.error("C li xy ra");
    }
  };
  const formatInputAmount = (value) => {
    const numbers = value.replace(/\D/g, "");
    return numbers.replace(/\B(?=(\d{3})+(?!\d))/g, ",");
  };
  return /* @__PURE__ */ jsxRuntimeExports.jsxs(MobileLayout, { showNav: false, children: [
    /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "p-4 space-y-6 mt-2", children: [
      /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "flex bg-secondary rounded-xl p-1", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(
          "button",
          {
            onClick: () => {
              setType("expense");
              setCategory("food");
