    className: "recharts-cartesian-grid"
  }, /*#__PURE__*/React$1.createElement(Background, {
    fill: propsIncludingDefaults.fill,
    fillOpacity: propsIncludingDefaults.fillOpacity,
    x: propsIncludingDefaults.x,
