	            // array
	            this.url.searchParams.append(column, `cs.{${value.join(',')}}`);
	        }
	        else {
	            // json
	            this.url.searchParams.append(column, `cs.${JSON.stringify(value)}`);
	        }
	        return this;
	    }
	    /**
