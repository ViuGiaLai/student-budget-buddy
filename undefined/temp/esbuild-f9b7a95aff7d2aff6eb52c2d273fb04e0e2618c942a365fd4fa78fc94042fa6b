  { id: "1", category: "food", limit: 2e6, period: "monthly", spent: 0 },
  { id: "2", category: "entertainment", limit: 5e5, period: "monthly", spent: 0 },
  { id: "3", category: "transport", limit: 3e5, period: "weekly", spent: 0 }
